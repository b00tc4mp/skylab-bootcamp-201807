    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "extensionHost",
            "request": "launch",
            "name": "Extensión de inicio",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "preLaunchTask": "npm"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Iniciar programa",
            "program": "${file}"
        }
    ]
}